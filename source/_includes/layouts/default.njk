<!doctype html>

<html>
<head>
  {%- include "global/head.njk" -%}
</head>
<body>

  <header class="header wrapper" id="header" aria-label="global site">
    <div class="container">
      {%- include "global/header.njk" -%}
    </div>
  </header>

  <main class="main wrapper" id="main" aria-label="body content">
    <div class="container">

      {% block title_content %}
        <h1>{{- title -}}</h1>
      {% endblock title_content %}

      {% block main_content %}
        {{ content | safe }}
      {% endblock main_content %}

    </div>
  </main>

  <footer class="footer wrapper" id="footer" aria-label="site information">
    <div class="container">
      {%- include "global/footer.njk" -%}
    </div>
  </footer>

<script async src="//embedr.flickr.com/assets/client-code.js" charset="utf-8"></script>
</body>
</html>
